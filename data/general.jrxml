	<queryString>
		<![CDATA[SELECT     
     MANIFESTE_SEGMENT_GENERAL.id AS id,
     MANIFESTE_SEGMENT_GENERAL.customs_office_code AS customs_office_code,
     MANIFESTE_SEGMENT_GENERAL.voyage_number AS voyage_number,
     MANIFESTE_SEGMENT_GENERAL.date_departure AS date_departure,
     MANIFESTE_SEGMENT_GENERAL.date_arrival AS date_arrival,
     MANIFESTE_SEGMENT_GENERAL.time_arrival AS time_arrival,
     MANIFESTE_SEGMENT_GENERAL.total_number_of_bols AS total_number_of_bols,
     MANIFESTE_SEGMENT_GENERAL.Total_number_of_packages AS Total_number_of_packages,
     MANIFESTE_SEGMENT_GENERAL.Total_number_of_containers AS Total_number_of_containers,
     MANIFESTE_SEGMENT_GENERAL.Total_gross_mass AS Total_gross_mass,
     MANIFESTE_SEGMENT_GENERAL.Identity_transporter AS Identity_of_transporter,
     MANIFESTE_SEGMENT_GENERAL.Carrier_code AS Carrier_code,
     MANIFESTE_SEGMENT_GENERAL.Carrier_name AS Carrier_name,
     MANIFESTE_SEGMENT_GENERAL.Carrier_address AS Carrier_address,
     MANIFESTE_SEGMENT_GENERAL.Place_of_departure_code AS Place_of_departure_code,
     MANIFESTE_SEGMENT_GENERAL.Place_of_destination_code AS Place_of_destination_code,
     MANIFESTE_SEGMENT_GENERAL.Tonnage_net_weight AS Tonnage_net_weight,
     MANIFESTE_SEGMENT_GENERAL.Tonnage_gross_weight AS Tonnage_gross_weight,
     MANIFESTE_BL.id AS id_bol,
     MANIFESTE_BL.id_general AS id_general,
     MANIFESTE_BL.Bol_reference AS Bol_reference,
     MANIFESTE_BL.Line_number AS Line_number,
     MANIFESTE_BL.Bol_nature AS Bol_nature,
     MANIFESTE_BL.Bol_type_code AS Bol_type_code,
     MANIFESTE_BL.Place_of_loading_code AS Place_of_loading_code,
     MANIFESTE_BL.Place_of_unloading_code AS Place_of_unloading_code,
     MANIFESTE_BL.Exporter_code AS Exporter_code,
     MANIFESTE_BL.Exporter_name AS Exporter_name,
     MANIFESTE_BL.Exporter_address AS Exporter_address,
     MANIFESTE_BL.Notify_code AS Notify_code,
     MANIFESTE_BL.Notify_name AS Notify_name,
     MANIFESTE_BL.Notify_address AS Notify_address,
     MANIFESTE_BL.Consignee_code AS Consignee_code,
     MANIFESTE_BL.Consignee_name AS Consignee_name,
     MANIFESTE_BL.Consignee_address AS Consignee_address,
     MANIFESTE_BL.Number_of_packages AS Number_of_packages,
     MANIFESTE_BL.Package_type_code AS Package_type_code,
     MANIFESTE_BL.Gross_mass AS Gross_mass,
     MANIFESTE_BL.Goods_description AS Goods_description,
     MANIFESTE_BL.Volume_in_cubic_meters AS Volume_in_cubic_meters,
     MANIFESTE_BL.Num_of_ctn_for_this_bol AS Num_of_ctn_for_this_bol
FROM
     MANIFESTE_SEGMENT_GENERAL MANIFESTE_SEGMENT_GENERAL INNER JOIN MANIFESTE_BL MANIFESTE_BL ON MANIFESTE_SEGMENT_GENERAL.id = MANIFESTE_BL.id_general
WHERE
     MANIFESTE_SEGMENT_GENERAL.id = $P{id}
ORDER BY 
MANIFESTE_BL.Bol_nature,
MANIFESTE_BL.Place_of_loading_code,
MANIFESTE_BL.Place_of_unloading_code]]>
	</queryString>
